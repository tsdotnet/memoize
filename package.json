{
  "name": "@tsdotnet/memoize",
  "version": "1.1.0",
  "description": "A utility for creating an iteratively cached (memoized) result of an iterable.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/Memoized.d.ts",
  "main": "dist/Memoized.js",
  "module": "dist/Memoized.mjs",
  "exports": {
    "import": "./dist/Memoized.mjs",
    "require": "./dist/Memoized.js"
  },
  "keywords": [
    "iterable",
    "iterator",
    "memoize",
    "memoized",
    "cached",
    "cached iterable"
  ],
  "scripts": {
    "build:cjs": "rimraf dist && tsc && copyfiles -u 1 src/**/*.d.ts dist",
    "build:esm": "rimraf temp-esm && tsc -p tsconfig.esm.json --outDir temp-esm && node -e \"require('fs').readdirSync('temp-esm').forEach(f=>{if(f.endsWith('.js')){require('fs').copyFileSync('temp-esm/'+f,'dist/'+f.replace('.js','.mjs'));} if(f.endsWith('.js.map')){require('fs').copyFileSync('temp-esm/'+f,'dist/'+f.replace('.js.map','.mjs.map'));}})\" && rimraf temp-esm",
    "build": "concurrently pnpm:build:cjs pnpm:build:esm",
    "docs": "rimraf docs && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "concurrently pnpm:lint pnpm:test && pnpm run build",
    "prepublishOnly": "pnpm run build && pnpm test",
    "preversion": "concurrently pnpm:lint pnpm:test",
    "postversion": "git push && git push --tags && pnpm run docs && git add -A && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/memoize.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/memoize/issues"
  },
  "homepage": "https://github.com/tsdotnet/memoize#readme",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.3.0",
    "chai": "^5.2.1",
    "concurrently": "^9.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.33.0",
    "mocha": "^11.7.1",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.10",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1"
  },
  "pnpm": {
    "overrides": {
      "hosted-git-info@<2.8.9": ">=2.8.9",
      "y18n@=4.0.0": ">=4.0.1",
      "lodash@<4.17.20": ">=4.17.20",
      "shell-quote@<=1.7.2": ">=1.7.3",
      "lodash@<4.17.21": ">=4.17.21",
      "json5@<1.0.2": ">=1.0.2",
      "json5@>=2.0.0 <2.2.2": ">=2.2.2",
      "minimatch@<3.0.5": ">=3.0.5",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "semver@>=7.0.0 <7.5.2": ">=7.5.2",
      "get-func-name@<2.0.1": ">=2.0.1",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "braces@<3.0.3": ">=3.0.3"
    }
  }
}
